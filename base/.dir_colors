SETGID  01;04                       # a file/directory with the setgid bit set
OTHER_WRITABLE 01;04;34             # an other-writable directory
STICKY  44                          # a directory with the sticky bit set
STICKY_OTHER_WRITABLE 01;04;44      # both of the above

